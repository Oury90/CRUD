<body>
    <div class="container">
        <h1>Jokes</h1>
        <% if(locals.dataJoke){ %>
            <p><%= dataJoke.joke %></p>
            <div class="content-input">
                <input type="text" placeholder="Id" name="id">
                <input type="text" placeholder="Joke" name="joke">
                <input type="text" placeholder="Author" name="author">
            </div>
            <a href="/">Get Joke</a>
            <a href="/form">Post Joke</a>
            <a href="/edit">Edit Joke</a>



       <% }else{%>
            <% console.error(); %>
        <% } %>
        
    </div>
    
